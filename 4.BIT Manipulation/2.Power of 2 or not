// Check weather Given No. is Power of 2 or not
//Explaination in notebook

#include <iostream>
using namespace std;
void isPowerof2(int n){
    if((n & (n-1))==0){
        cout<<"yes";
    }
    else{
    cout<<"no";
    }
}
int main() {
    
    int n;
    cin>>n;
    isPowerof2(n);

    return 0;
}
