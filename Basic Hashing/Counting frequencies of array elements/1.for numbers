#include <bits/stdc++.h>
using namespace std;

int main() {
    int n;
    cin>>n;
    int arr[n];
    
    for(int i=0;i<n;i++) cin>>arr[i];
    
    map<int, int> mpp;//map<char, int> map for characters frequency 
    for(int i=0;i<n;i++) mpp[arr[i]]++;
    
    int q;
    cin>>q;
    while(q--){
        int n;
        cin>>n;
        
        cout<<mpp[n]<<endl;
    }

    return 0;
}
