// Online C++ compiler to run C++ program online
#include <bits/stdc++.h>
using namespace std;

//Time Complexity = O(log n)
void count1(int n){
    int count = 0;
    while(n>0){
        int lastdigit = n%10;
        count++;
        n = n/10;
    }
    
    cout<<count<<endl;
}

//Time Complexity = O(1)
void count2(int n){
    
    int count = log10(n)+1;
    
    cout<<count<<endl;
}

int main() {
    
    int n;
    cin>>n;
    
    count1(n);
    count2(n);

    return 0;
}
