#include <bits/stdc++.h>
using namespace std;

//Time Complexity = O(log n)
void palindrome(int n){
    int temp = n;
    int ans=0;
    while(n>0){
        ans  = ans*10 + n%10;
        n = n/10;
    }
    
    if(temp == ans) cout<<"True"<<endl;
    else cout<<"False"<<endl;
}


int main() {
    
    int n;
    cin>>n;
    
    palindrome(n);

    return 0;
}
