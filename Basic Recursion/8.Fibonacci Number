          F(4)
         /    \
      F(3)    F(2)
     /   \    /   \
  F(2)  F(1) F(1) F(0)
  /  \
F(1) F(0)

Time Complexity: O(2^n)
Space Complexity: O(n)


#include <iostream>
using namespace std;

int Fibonacci(int n){
    if(n <= 1) return n;
    
    int last = Fibonacci(n-1);
    int slast = Fibonacci(n-2);
    return last+slast;
    
}

int main() {
    
    cout<<Fibonacci(4);

    return 0;
}

	
