#include <iostream>
using namespace std;

int LargestElement(int arr[],int n){
    int l = arr[0];
    
    for(int i=1;i<n;i++){
        if(arr[i] > l) l = arr[i];
    }
    
    return l;
}

int main() {
    
    int n;
    cin>>n;
    int arr[n];
    
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    
    cout<<LargestElement(arr,n);

    return 0;
}
