#include <iostream>
using namespace std;

int missing(int arr[], int n) {
    int sum2 = 0;
    int sum1 = (n + 1) * (n + 2) / 2; // sum of 1 to n+1
    
    for(int i = 0; i < n; i++) {
        sum2 += arr[i];
    }
    
    return sum1 - sum2;
}

int main() {
    int n;
    cin >> n; // number of elements in array (one missing from 1..n+1)
    int arr[n];
    
    for(int i = 0; i < n; i++) {
        cin >> arr[i];
    }
    
    cout << missing(arr, n);

    return 0;
}
