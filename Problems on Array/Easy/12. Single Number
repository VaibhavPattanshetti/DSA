/*
Given a non-empty array of integers nums, every element appears twice except for one. Find that single one.


XOR between 2 same numbers will 0 
otherwise same number as it is

Example 1:
Input: nums = [2,2,1]
Output: 1
Example 2:
Input: nums = [4,1,2,1,2]
Output: 4

*/

#include <bits/stdc++.h>
using namespace std;

int SingleNumber(int arr[],int n){
    int XOR = 0;
    
    for(int i=0;i<n;i++){
        XOR = XOR ^ arr[i];
    }
    
    return XOR;
}

int main() {
    
    int n;
    cin>>n;
    int arr[n];
    
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    
    cout<<SingleNumber(arr,n);

    return 0;
}
