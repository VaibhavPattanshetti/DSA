#include <bits/stdc++.h>
using namespace std;

vector<int> unionArr(int arr1[],int arr2[],int n1,int n2){
    int i=0,j=0;
    vector<int> unionArr;
    
    while(i<n1 && j<n2){
        if(arr1[i] <= arr2[j]){
            if(unionArr.size() == 0 || unionArr.back() != arr1[i]){
                unionArr.push_back(arr1[i]);
            }
            i++;
        }else{
            if(unionArr.size() == 0 || unionArr.back() != arr2[j]){
                unionArr.push_back(arr2[j]);
            }
            j++;
        }
    }
    
    while(i<n1){
        if(unionArr.size() == 0 || unionArr.back() != arr1[i]){
                unionArr.push_back(arr1[i]);
            }
            i++;
    }
    while(j<n2){
        if(unionArr.size() == 0 || unionArr.back() != arr2[j]){
                unionArr.push_back(arr2[j]);
            }
            j++;
    }
    
    return unionArr;
    
    //T.C = O(n1+n2)
    //S.C = O(n1+n2) ;this space is or returing purpose ,not the part of algo
} 

int main() {
    int n1,n2;
    cin>>n1>>n2;
    int arr1[n1], arr2[n2];
    
    for(int i=0;i<n1;i++) cin>>arr1[i];
    for(int i=0;i<n2;i++) cin>>arr2[i];
    
    vector<int> uni = unionArr(arr1,arr2,n1,n2);
    
    for(int i : uni) cout<<i<<" ";

    return 0;
}
